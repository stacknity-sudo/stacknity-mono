{
  "name": "@stacknity/shared-ui",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs",
      "default": "./src/index.ts"
    },
    "./dist/styles.css": "./dist/index.css",
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "./dist/index.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "echo 'lint shared-ui'",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ui",
    "components",
    "design-system"
  ],
  "author": "Stacknity",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "5.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
